{
  "name": "atd-introduction-to-cypress",
  "version": "1.0.0",
  "description": "An introductory workshop about Cypress",
  "main": "index.js",
  "scripts": {
    "test": "cypress run --spec 'cypress/integration/06-ci/*'",
    "posttest": "npm run --prefix todomvc reset:db",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "start": "npm start --prefix todomvc -- --quiet",
    "ci": "start-test 3001",
    "dev": "start-test 3001 cy:open",
    "postinstall": "cd todomvc && npm install",
    "reset": "npm run reset --prefix todomvc",
    "format": "eslint --fix 'cypress/**/*.js'",
    "lint": "eslint 'cypress/**/*.js'",
    "slides:dev": "vite",
    "slides:build": "vite build"
  },
  "keywords": [],
  "author": "Marie Drake",
  "license": "ISC",
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "cypress": "9.6.0",
    "cypress-mochawesome-reporter": "^3.0.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-prettier": "3.1.2",
    "highlight.js": "10.7.2",
    "http-server": "^14.1.0",
    "mocha": "^6.2.3",
    "mocha-junit-reporter": "1.23.3",
    "mocha-multi-reporters": "1.1.7",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "4.2.0",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "2.2.1",
    "reveal.js": "^4.3.1",
    "start-server-and-test": "1.12.1",
    "vite": "2.1.5"
  },
  "engines": {
    "node": ">=12"
  }
}
